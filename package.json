{
  "name": "hackernews",
  "version": "0.9.0",
  "description": "A hacker news clone built from the ground up to demonstrate React and GraphQL",
  "engines": {
    "node": ">=10.15.3"
  },
  "scripts": {
    "clean": "rm -rf build/app && rm -rf dist && rm -rf src/.next",
    "build": "npm run clean && tsc && next build",
    "build:docker": "docker build --tag 'clintonwoo/hackernews-react-graphql:latest' --rm . && docker run -p 80:3000 --name hackernews-react-graphql clintonwoo/hackernews-react-graphql",
    "build:static-website": "NODE_ENV=production rm -rf build/static && npm install --only=dev && next build && next export -o build/static",
    "debug": "DEBUG=app:* npm start",
    "debug:inspect": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} node --inspect -r ts-node/register src/server.ts",
    "debug:inspect-production": "NODE_ENV=production node --inspect dist/server.js",
    "debug:inspectr": "NODE_ENV=production DEBUG=app* ts-node --inspect dist/server.js",
    "debug:all": "DEBUG=* npm start",
    "lint": "eslint \"src/**/*.ts?(x)\" --ext .js,.jsx,.ts,.tsx",
    "nodemon": "nodemon src/server.ts --exec ts-node",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "test": "jest --config jest.config.js",
    "tsc:check": "tsc --noEmit",
    "start": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node src/server.ts",
    "start:prod": "NODE_ENV=production node dist/server.js"
  },
  "author": "Clinton D'Annolfo",
  "license": "Apache-2.0",
  "keywords": [
    "hacker-news",
    "clone",
    "react",
    "graphql"
  ],
  "dependencies": {
    "apollo-cache-inmemory": "1.6.5",
    "apollo-client": "2.6.8",
    "apollo-fetch": "0.7.0",
    "apollo-link-http": "1.5.16",
    "apollo-server-express": "2.11.0",
    "body-parser": "1.19.0",
    "cookie": "0.4.0",
    "cookie-parser": "1.4.5",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "firebase": "7.12.0",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "graphql-tools": "4.0.7",
    "isomorphic-unfetch": "3.0.0",
    "lru-cache": "5.1.1",
    "next": "9.3.1",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "react": "16.13.1",
    "react-apollo": "3.1.3",
    "react-dom": "16.13.1",
    "react-render-html": "0.6.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@types/async": "3.0.8",
    "@types/body-parser": "1.19.0",
    "@types/cookie": "0.3.3",
    "@types/cookie-parser": "1.4.2",
    "@types/debug": "4.1.5",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/express": "4.17.3",
    "@types/express-session": "1.17.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "25.1.4",
    "@types/lru-cache": "5.1.0",
    "@types/node": "13.9.3",
    "@types/passport": "1.0.3",
    "@types/passport-local": "1.0.33",
    "@types/react": "16.9.25",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/eslint-plugin-tslint": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "@zeit/next-css": "1.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-define": "2.0.0",
    "cross-env": "7.0.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "7.2.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "jest": "25.1.0",
    "jest-enzyme": "7.1.2",
    "mockdate": "2.0.5",
    "nodemon": "2.0.2",
    "postcss": "7.0.27",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.1",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "src/.next*",
      "src/pages*"
    ],
    "delay": "10",
    "verbose": false,
    "watch": [
      "src"
    ]
  }
}
